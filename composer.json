{
    "name": "codelikes/lookupvalue",
    "description": "",
    "keywords": [
    ],
    "homepage": "https://github.com/codelikes/LookupValue",
    "license": "MIT",
    "authors": [
        {
            "name": "Nikolay Ponich",
            "email": "n.ponich@gmail.com",
            "homepage": "https://cv.ponich.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/container": "^5.8|^6.0|^7.0|^8.0",
        "illuminate/contracts": "^5.8|^6.0|^7.0|^8.0",
        "illuminate/database": "^5.8|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8|^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Codelikes\\LookupValue\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Codelikes\\LookupValue\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Codelikes\\LookupValue\\LookupValueServiceProvider"
            ],
            "aliases": {
                "LookupValue": "Codelikes\\LookupValue\\Facade"
            }
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
